{
  "corsConfig": {
    "enabled": true,
    "allowCredentials": true,
    "unsafeWildcardOriginWithAllowCredentials": true,
    "allowedOrigins": [
      "https://${DOMAIN}",
      "http://localhost1.com:3000",
      "http://localhost1.com:3001",
      "http://localhost:3000",
      "http://localhost:3001",
      "http://localhost:7090",
      "http://localhost:5108",
      "http://localhost:5080",
      "http://localhost:9045",
      "http://localhost:9046",
      "http://localhost:7080"
    ]
  },
  "emailVerificationRequired": true,
  "multiFactorRequired": true,
  "multiFactorRequiredByEmailCode": true,
  "oidcConfig": {
    "baseUrl": "https://${DOMAIN}"
  },
  "selfIDPConfig": {
    "authority": "https://${DOMAIN}",
    "redirectUrl": "https://${DOMAIN}/auth/callback"
  },
  "systemConfig": {
    "domain": "${DOMAIN}",
    "developerMode": false
  },
  "sessionConfig": {
    "domain": "${COOKIE_DOMAIN}"
  },
  "cookieConfig": {
    "domain": "${COOKIE_DOMAIN}"
  },
  "totp": {
    "enabled": false,
    "issuerName": "EXAMPLE.RAGE.IDENTITY"
  },
  "urlRewritesConfig": {
    "enabled": true,
    "rules": [
      {
        "from": "/",
        "to": "/management/"
      },
      {
        "from": "/home",
        "to": "/management/"
      }
    ]
  },
  "webAuthNConfig": {
    "rpDisplayName": "RAGE",
    "rpID": "${DOMAIN}",
    "rpOrigins": [
      "https://${DOMAIN}"
    ]
  }
}